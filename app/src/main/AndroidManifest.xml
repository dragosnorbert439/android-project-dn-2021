<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidprojectdn2021" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_colored_v2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_colored_v2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidProjectDN2021.Bazaar" >
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MarketActivity"
            android:label="@string/app_name" />
    </application>

</manifest>